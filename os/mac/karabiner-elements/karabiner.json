{
  "global" : {
    "check_for_updates_on_startup" : true,
    "show_in_menu_bar" : true,
    "show_profile_name_in_menu_bar" : false
  },
  "profiles" : [ {
    "complex_modifications" : {
      "parameters" : {
        "basic.simultaneous_threshold_milliseconds" : 50,
        "basic.to_delayed_action_delay_milliseconds" : 30,
        "basic.to_if_alone_timeout_milliseconds" : 20,
        "basic.to_if_held_down_threshold_milliseconds" : 100
      },
      "rules" : [ {
        "description" : "[launch] Firefox",
        "manipulators" : [ {
          "from" : {
            "key_code" : "f",
            "modifiers" : {
              "mandatory" : [ "left_command", "left_control", "left_option" ]
            }
          },
          "to" : [ {
            "shell_command" : "osascript -e 'tell application \"Firefox\" to activate'"
          } ],
          "type" : "basic"
        } ]
      }, {
        "description" : "[launch] Google Chrome",
        "manipulators" : [ {
          "from" : {
            "key_code" : "c",
            "modifiers" : {
              "mandatory" : [ "left_command", "left_control", "left_option" ]
            }
          },
          "to" : [ {
            "shell_command" : "osascript -e 'tell application \"Google Chrome\" to activate'"
          } ],
          "type" : "basic"
        } ]
      }, {
        "description" : "[chrome] Show recent tabs",
        "manipulators" : [ {
          "from" : {
            "modifiers" : {
              "mandatory" : [ "left_command" ]
            },
            "key_code" : "e"
          },
          "to" : [ {
            "key_code" : "a",
            "modifiers" : [ "left_command", "left_shift" ]
          } ],
          "conditions" : [ {
            "type" : "frontmost_application_if",
            "bundle_identifiers" : [ "com.google.Chrome", "com.google.Chrome.beta" ]
          } ],
          "type" : "basic"
        } ]
      }, {
        "description" : "[chrome] Inspect element",
        "manipulators" : [ {
          "from" : {
            "key_code" : "i",
            "modifiers" : {
              "mandatory" : [ "left_command", "left_shift" ]
            }
