          "to" : [ {
            "key_code" : "c",
            "modifiers" : [ "left_command", "left_shift" ]
          } ],
          "conditions" : [ {
            "type" : "frontmost_application_if",
            "bundle_identifiers" : [ "com.google.Chrome", "com.google.Chrome.beta" ]
          } ],
          "type" : "basic"
